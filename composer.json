{
  "name": "wrux/blocky",
  "description": "Map a matrix field into an array of blocks to render in twig.",
  "type": "craft-plugin",
  "version": "1.1.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "block parser"
  ],
  "support": {
    "docs": "https://github.com/wrux/blocky/blob/master/README.md",
    "issues": "https://github.com/wrux/blocky/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Callum Bonnyman",
      "homepage": "https://bloke.blog"
    }
  ],
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse ./src --level=5"
  },
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "wrux\\blocky\\": "src/"
    }
  },
  "extra": {
    "name": "Blocky",
    "handle": "blocky",
    "developer": "Callum Bonnyman",
    "developerUrl": "https://bloke.blog",
    "documentationUrl": "https://github.com/wrux/blocky/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/wrux/blocky/master/CHANGELOG.md",
    "class": "wrux\\blocky\\Blocky"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0.0"
  },
  "config": {
    "platform": {
      "php": "8.0.0"
    }
  }
}
